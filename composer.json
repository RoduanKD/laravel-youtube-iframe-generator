{
    "name": "syrian-open-source/laravel-youtube-iframe-generator",
    "description": "Laravel package allows you to generate an iframe tag with a video player depending on a youtube URL.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "syrian open source",
        "laravel-iframe",
        "youtube-iframe"
    ],
    "authors": [
        {
            "name": "Abdussalam Al-Ali",
            "email": "alali.abdusslam@gmail.com",
            "homepage": "https://github.com/abdussalam-alali",
            "role": "Developer"
        }
    ],
    "require": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload" : {
        "psr-4": {
            "SOS\\LaravelYoutubeFrameGenerator\\" : "src",
            "SOS\\LaravelYoutubeFrameGenerator\\Tests\\": "src/tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "SOS\\LaravelYoutubeFrameGenerator\\Providers\\YFrameServiceProviders"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "orchestra/testbench": "^6.23"
    }
}
